### debirf.conf defaults

# label
DEBIRF_LABEL=${DEBIRF_LABEL:-"debirf"}

# where to build the debirf
DEBIRF_BUILDD=${DEBIRF_BUILDD:-"$DEBIRF_PROFILE"}

# debirf boot method
DEBIRF_METHOD=${DEBIRF_METHOD:-"nested"}

# Debian mirror
DEBIRF_MIRROR=${DEBIRF_MIRROR:-"http://mirrors.kernel.org/debian"}

# Debian suite
DEBIRF_SUITE=${DEBIRF_SUITE:-"lenny"}

# Debian debootstrap verification keyring
DEBIRF_KEYRING=${DEBIRF_KEYRING:-"/usr/share/keyrings/debian-archive-keyring.gpg"}
